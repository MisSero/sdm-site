@model ServiceItemViewModel
@{
	ViewBag.Title = Model.MetaTitle;
	ViewBag.Description = Model.MetaDescription;
	ViewBag.Keyword = Model.MetaKeywords;
}
<div>
	<h2>
		@Model.Title
	</h2>
	<h3>
		@Model.Subtitle
	</h3>
	<div>
		<img class="img-block" src="~/images/@Model.TitleImagePath" />
	</div>
</div>
<div>
	@Html.Raw(Model.Text)
</div>